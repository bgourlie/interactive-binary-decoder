{
  "name": "wtfloat",
  "version": "0.1.0",
  "description": "An interactive guide to floating point numbers",
  "scripts": {
    "dev": "parcel ./src/root.html --no-hmr",
    "dev-debug": "node %NODE_DEBUG_OPTION% ./node_modules/parcel-bundler/bin/cli.js ./src/root.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build ./src/root.tsx --out-dir app_dist",
    "build_debug": "parcel build ./src/root.tsx --out-dir app_dist --no-minify",
    "build_ssr": "parcel build ./src/ssr_handler.ts --no-minify --out-dir serverless_dist",
    "deploy": "npm run build_ssr && serverless deploy"
  },
  "keywords": [],
