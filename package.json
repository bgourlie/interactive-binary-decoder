    "@types/aws-lambda": "0.0.22",
    "@types/exenv": "^1.2.0",
    "@types/history": "^4.6.2",
    "@types/node": "^8.0.57",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/shelljs": "^0.7.8",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.7.1",
    "shelljs": "^0.8.1",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "aphrodite": "^2.2.0",
    "cheerio": "^1.0.0-rc.2",
    "exenv": "^1.2.2",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  }
}
